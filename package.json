{
	"name": "rivet",
	"version": "0.0.1",
	"private": true,
	"author": "Rivet Gaming, LLC",
	"license": "GNU-3.0",
	"scripts": {
		"docusaurus": "docusaurus",
		"start": "docusaurus clean-api-docs all && docusaurus gen-api-docs all && docusaurus start --host 0.0.0.0 --port 6600",
		"build": "docusaurus build",
		"swizzle": "docusaurus swizzle",
		"deploy": "docusaurus deploy",
		"clear": "docusaurus clear",
		"serve": "docusaurus serve --host 0.0.0.0 --port 6600",
		"write-translations": "docusaurus write-translations",
		"write-heading-ids": "docusaurus write-heading-ids",
		"typecheck": "tsc"
	},
	"dependencies": {
		"@docusaurus/core": "2.0.0-beta.21",
		"@docusaurus/preset-classic": "2.0.0-beta.21",
		"@easyops-cn/docusaurus-search-local": "^0.29.0",
		"@mdx-js/react": "^1.6.22",
		"clsx": "^1.1.1",
		"docusaurus-plugin-openapi-docs": "^1.0.6",
		"docusaurus-theme-openapi-docs": "^1.0.6",
		"prism-react-renderer": "^1.3.3",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@docusaurus/module-type-aliases": "2.0.0-beta.21",
		"@tsconfig/docusaurus": "^1.0.5",
		"typescript": "^4.6.4"
	},
	"browserslist": {
		"production": [
			">0.5%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"prettier": {
		"trailingComma": "none",
		"tabWidth": 4,
		"useTabs": true,
		"singleQuote": true,
		"printWidth": 110,
		"endOfLine": "lf",
		"arrowParens": "avoid",
		"bracketSpacing": true,
		"jsxBracketSameLine": true,
		"jsxSingleQuote": true
	}
}
